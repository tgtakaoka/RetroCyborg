10 REM ****************************20 REM * ASCII art Mandelbrot set *30 REM *   Fixed floating point   *31 REM * https://blog.goo.ne.jp/tk-80/e/1f1552542b63ed4c72860fe69b962ba840 REM ****************************50 F=5051 REM F is '1.0'59 Y=-1260 REM FOR Y=-12 TO 1269 X=-4970 REM FOR X=-49 TO 2980 C=X*229/10081 REM 0.0458*F90 D=Y*416/10091 REM 0.0833*F100 A=C110 B=D120 I=0130 Q=B/F: S=B-Q*F140 T=(A*A-B*B)/F+C150 B=2*(A*Q+A*S/F)+D160 A=T170 P=A/F: Q=B/F180 IF (P*P+Q*Q)>4 THEN 220190 I=I+1: IF I<=15 THEN 130200 PRINT " ";210 GOTO 240220 IF I>9 THEN I=I+7230 GO@ %61, I+48240 X=X+1: IF X<=29 THEN 70250 PRINT260 Y=Y+1: IF Y<=12 THEN 60